<objdefs>
   <obj.normal id="queuestate" sha="285ac1c9764f8f7b721aa750f5bd63f63e2578d5">
      <sDescription>Gets the number of pending bytes in the midi output queue. Useful to prevent midi data flooding. Zero at rest.</sDescription>
      <author>Johannes Taelman</author>
      <license>BSD</license>
      <inlets/>
      <outlets>
         <int32 name="length" description="number of pending bytes in queue" SumBuffer="false"/>
      </outlets>
      <displays/>
      <params/>
      <attribs>
         <combo name="device">
            <MenuEntries>
               <string>din</string>
               <string>usb host</string>
               <string>internal</string>
            </MenuEntries>
            <CEntries>
               <string>1</string>
               <string>3</string>
               <string>15</string>
            </CEntries>
         </combo>
      </attribs>
      <code.krate><![CDATA[%length% = MidiGetOutputBufferPending((midi_device_t) %device%);
]]></code.krate>
   </obj.normal>
</objdefs>